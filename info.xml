<?xml version="1.0"?>
<extension key="com.klangsoft.overrides" type="module">
  <file>overrides</file>
  <name>Extension File Overrides</name>
  <description>Monitors for changes made to core files that are overridden by an extension, and provides tools to deal with it.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>David Reedy Jr</author>
    <email>davidrjr.konadave@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/konadave/com.klangsoft.overrides</url>
    <url desc="Documentation">https://github.com/konadave/com.klangsoft.overrides</url>
    <url desc="Support">https://github.com/konadave/com.klangsoft.overrides/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-05-25</releaseDate>
  <version>2.0-beta1</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Perishment is choice, than a wretched life</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Overrides</namespace>
    <format>22.10.0</format>
    <angularModule>crmOverrides</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
