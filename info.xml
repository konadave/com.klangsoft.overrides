<?xml version="1.0"?>
<extension key="com.klangsoft.overrides" type="module">
  <file>overrides</file>
  <name>Extension File Overrides</name>
  <description>Displays the files each extension overrides and whether or not the core files have been modified since last check. This makes it much easier to know which files will need some diff/merge to make the extensions compatible with the latest core files.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>David Reedy Jr, Klangsoft LLC</author>
    <email>dreedy@klangsoft.com</email>
  </maintainer>
  <urls>
    <url desc="Extension Page">/civicrm/overrides</url>
    <url desc="Download Page">https://github.com/konadave/com.klangsoft.overrides</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2015-09-14</releaseDate>
  <version>1.0</version>
  <develStage>production</develStage>
  <compatibility>
    <ver>4.6</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Overrides</namespace>
  </civix>
</extension>
